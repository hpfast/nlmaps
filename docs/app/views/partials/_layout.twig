<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>NL Maps</title>
    <link rel="apple-touch-icon" sizes="57x57" href="assets/favicon/apple-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="assets/favicon/apple-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="assets/favicon/apple-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="assets/favicon/apple-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="assets/favicon/apple-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="assets/favicon/apple-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="assets/favicon/apple-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="assets/favicon/apple-icon-152x152.png">
    <link rel="apple-touch-icon" sizes="180x180" href="assets/favicon/apple-icon-180x180.png">
    <link rel="icon" type="image/png" sizes="192x192"  href="assets/favicon/android-icon-192x192.png">
    <link rel="icon" type="image/png" sizes="32x32" href="assets/favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="96x96" href="assets/favicon/favicon-96x96.png">
    <link rel="icon" type="image/png" sizes="16x16" href="assets/favicon/favicon-16x16.png">
    <link rel="manifest" href="assets/favicon/manifest.json">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="assets/favicon/ms-icon-144x144.png">
    <meta name="theme-color" content="#ffffff">
    <link href="app.css" rel="stylesheet" type="text/css" />
    <link href="assets/fonts/icon-font/css/fontello.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" type="text/css" href="https://unpkg.com/leaflet@1.0.3/dist/leaflet.css" />
    <link rel="stylesheet" type="text/css" href="https://api.mapbox.com/mapbox.js/v3.1.0/mapbox.css" />
    <link rel="stylesheet" type="text/css" href="https://openlayers.org/en/v4.1.0/css/ol.css" />
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.5.18/webfont.js"></script>
    <script>
        WebFont.load({
            google: {
                families: ['Montserrat:400', 'Nunito:400,400i,600,600i']
            }
        });
    </script>
</head>
<body{% block body_attributes %}{% endblock %}>
    <div class="wrapper">
        {% block header %}
            {% include '_header.twig' %}
        {% endblock %}
        {% block content %}{% endblock %}
        {% block footer %}
            {% include '_footer.twig' %}
        {% endblock %}
    </div>
    <script type="text/template" id="forum-item">
        <li class="forum-item">
            <article class="faq">
                <a href="{link}" target="_blank" class="faq-link">
                    <div class="faq-content">
                        <h1 class="heading faq-heading">{title}</h1>
                        <span class="faq-date">Laatste reactie op {date}</span>
                    </div>
                    <div class="faq-metas">
                        <span class="faq-meta faq-comments">{comments}</span>
                        <span class="faq-meta faq-upvotes">{likes}</span>
                    </div>
                </a>
            </article>
        </li>
    </script>
    <script type="text/template" id="google-maps-template">
&lt;div id="nlmaps-holder">&lt;/div>

&lt;script src="https://maps.googleapis.com/maps/api/js?v=3&key=YOUR-API-KEY">&lt;/script>
&lt;script type="text/javascript">
    var nlMapsHolder = document.getElementById('nlmaps-holder');
    nlMapsHolder.style.height = '300px'; // Change to wanted height

    var map = new google.maps.Map(nlMapsHolder, {
        center: {
            lat: {latitude},
            lng: {longitude}
        },
        zoom: {zoomlevel}
    });

    var nlMap = new google.maps.ImageMapType({
        getTileUrl: (coord, zoom) => {
            let url = '{url}/' + zoom + '/' + coord.x + '/' + coord.y + '.{extension}';
            return url;
        },
        tileSize: new google.maps.Size(256, 256),
        isPng: true,
        name: 'NLMaps',
        maxZoom: 18,
        minZoom: 8
    });

    map.mapTypes.set('NLmaps', nlMap);
    map.setOptions({
        mapTypeControl: false,
    });

    map.setMapTypeId('NLmaps');
&lt;/script>
    </script>
    <script type="text/template" id="leaflet-template">
&lt;div id="nlmaps-holder">&lt;/div>

&lt;script src="https://unpkg.com/leaflet@1.0.3/dist/leaflet.js">&lt;/script>
&lt;link href="https://unpkg.com/leaflet@1.0.3/dist/leaflet.css" rel="stylesheet" type="text/css" />
&lt;script type="text/javascript">
    var nlMapsHolder = document.getElementById('nlmaps-holder');
    nlMapsHolder.style.height = '300px'; // Change to wanted height

    var map = L.map('nlmaps-holder', { // ID of map div
        center: [{latitude}, {longitude}],
        layers: [
            new L.tileLayer(
                '{url}/{z}/{x}/{y}.{extension}', {
                    attribution: '{attribution}'
                }
            )
        ],
        zoom: {zoomlevel}
    });
&lt;/script>
    </script>
    <script type="text/template" id="open-layers-template">
&lt;div id="nlmaps-holder">&lt;/div>

&lt;script src="https://openlayers.org/en/v4.1.0/build/ol.js">&lt;/script>
&lt;link href="https://openlayers.org/en/v4.1.0/css/ol.css" rel="stylesheet" type="text/css" />
&lt;script type="text/javascript">
    var nlMapsHolder = document.getElementById('nlmaps-holder');
    nlMapsHolder.style.height = '300px'; // Change to wanted height

    var map = new ol.Map({
        layers: [
            new ol.layer.Tile({
                source: new ol.source.XYZ({
                    url: '{url}/{z}/{x}/{y}.{extension}',
                    attributions: [
                        new ol.Attribution({
                            html: '{attribution}'
                        })
                    ]
                })
            })
        ],
        view: new ol.View({
            center: ol.proj.fromLonLat([{longitude}, {latitude}]),
            zoom: {zoomlevel}
        }),
        target: 'nlmaps-holder' // ID of map div
    });
&lt;/script>
    </script>
    <script type="text/template" id="mapbox-template">
&lt;div id="nlmaps-holder">&lt;/div>

&lt;script src="https://api.mapbox.com/mapbox.js/v3.1.0/mapbox.js">&lt;/script>
&lt;link href="https://api.mapbox.com/mapbox.js/v3.1.1/mapbox.css" rel="stylesheet" type="text/css" />
&lt;script type="text/javascript">
    var nlMapsHolder = document.getElementById('nlmaps-holder');
    nlMapsHolder.style.height = '300px'; // Change to wanted height

    var map = L.mapbox
        .map('nlmaps-holder') // ID of map div
        .setView([{latitude}, {longitude}], {zoomlevel});

    var layer = L.tileLayer('{url}/{z}/{x}/{y}.{extension}', {
        tms: false,
        attribution: '{attribution}'
    });
    layer.addTo(map);
&lt;/script>
    </script>

    <script type="text/javascript" src="https://unpkg.com/leaflet@1.0.3/dist/leaflet.js"></script>
    <script type="text/javascript" src="https://api.mapbox.com/mapbox.js/v3.1.0/mapbox.js"></script>
    <script type="text/javascript" src="https://openlayers.org/en/v4.1.0/build/ol.js"></script>
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?v=3&key=AIzaSyAHhkVptxb8zlX0LZqkLf-RHC9Uvik_fDc"></script>
    <script type="text/javascript" src="app.js" async defer></script>
</body>
</html>
